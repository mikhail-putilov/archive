        <mrow>
         <mi>T</mi>
        </mrow>
        <annotation encoding="StarMath 5.0">T</annotation>
       </semantics>
      </math>
     </draw:object><draw:image>
      <office:binary-data>VkNMTVRGAgAyAAAAAAAAAAEAGwAAAAAAAAAAAAAAAAABAAAAAQAAAAEAAAABAAAAAeIBAADU
